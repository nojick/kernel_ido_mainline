&pm8916_bms {
	status = "okay";
	monitored-battery = <&battery>;
};

&pm8916_pwm {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pwm_out>;
};

&pm8916_resin {
	status = "okay";
	linux,code = <KEY_VOLUMEDOWN>;
};

&pm8916_vib {
	status = "okay";
};

&wcnss {
	status = "okay";
};

&wcnss_mem {
	status = "okay";
};

&wcnss_iris {
	compatible = "qcom,wcn3620";
};

&sdhc_1 {
	status = "okay";
};

&sdhc_2 {
	status = "okay";

	pinctrl-0 = <&sdc2_default &sdc2_cd_default>;
	pinctrl-1 = <&sdc2_sleep &sdc2_cd_default>;
	pinctrl-names = "default", "sleep";

	cd-gpios = <&tlmm 38 GPIO_ACTIVE_HIGH>;
};

&usb {
	status = "okay";
	extcon = <&usb_id>, <&usb_id>;
	vbus-supply = <&usb_vbus>;
};

&usb_hs_phy {
	extcon = <&usb_id>;
	status = "okay";
};


&pm8916_rpm_regulators {
		l1 {
			regulator-min-microvolt = <1225000>;
			regulator-max-microvolt = <1225000>;
		};

		pm8916_l4: l4 {
			regulator-min-microvolt = <2050000>;
			regulator-max-microvolt = <2050000>;
		};

		pm8916_l10: l10 {
			regulator-min-microvolt = <2700000>;
			regulator-max-microvolt = <2800000>;
		};

		pm8916_l14: l14 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <3300000>;
		};

		pm8916_l15: l15 {
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <3300000>;
		};

		pm8916_l16: l16 {
			regulator-min-microvolt = <2600000>;
			regulator-max-microvolt = <2600000>;
		};

		pm8916_l17: l17 {
			regulator-min-microvolt = <2850000>;
			regulator-max-microvolt = <2850000>;
            regulator-always-on;
		};

		pm8916_l18: l18 {
			regulator-min-microvolt = <2700000>;
			regulator-max-microvolt = <2700000>;
		};
	};

&tlmm {
	usb_id_default: usb-id-default {
		pins = "gpio110";
		function = "gpio";

		drive-strength = <8>;
		bias-pull-up;
	};

	ext_buck_vsel_default: ext_buck_vsel {
		pins = "gpio111";
		function = "gpio";

		drive-strength = <8>;
		bias-pull-up;
	};

smb_int_default: smb-int-default-state {
		pins = "gpio62";
		function = "gpio";

		drive-strength = <2>;
		bias-pull-up;
	};

    mdss {
		mdss_default: mdss-default {
			pins = "gpio25";
			function = "gpio";

			drive-strength = <8>;
			bias-disable;
		};
		mdss_sleep: mdss-sleep {
			pins = "gpio25";
			function = "gpio";

			drive-strength = <2>;
			bias-pull-down;
		};
	};

    bma2x2_int1_pin {
		bma2x2_int1_default: int1_default {
			pins = "gpio112";
			function = "gpio";

			drive-strength = <6>;
			bias-pull-up;
		};
		bma2x2_int1_sleep: int1_sleep {
			pins = "gpio112";
			function = "gpio";

			drive-strength = <2>;
			bias-pull-down;
		};
	};

	bma2x2_int2_pin {
		bma2x2_int2_default: int2_default {
			pins = "gpio114";
			function = "gpio";

			drive-strength = <6>;
			bias-pull-up;
		};
		bma2x2_int2_sleep: int2_sleep {
			pins = "gpio114";
			function = "gpio";

			drive-strength = <2>;
			bias-pull-down;
		};
	};

    ltr559_int_pin {
		alsps_default: alsps_default {
			pins = "gpio113";
			function = "gpio";

			drive-strength = <6>;
			bias-pull-up;
		};
		alsps_sleep: alsps_sleep {
			pins = "gpio113";
			function = "gpio";

			drive-strength = <2>;
			bias-pull-down;
		};
};

    mpu6050_int_pin {
		mpu6050_default: mpu6050_default {
			pins = "gpio115";
			function = "gpio";

			drive-strength = <6>;
			bias-pull-up;
		};
		mpu6050_sleep: mpu6050_sleep {
			pins = "gpio115";
			function = "gpio";

			drive-strength = <2>;
			bias-pull-down;
		};
	};

touchscreen_default: touchscreen-default {
		pins = "gpio13";
		function = "gpio";

		drive-strength = <2>;
		bias-pull-up;

		reset {
			pins = "gpio12";
			function = "gpio";

			drive-strength = <2>;
			bias-disable;
		};
	};

	tps65132_en_default: tps65132-en-default {
		pins = "gpio97", "gpio98";
		function = "gpio";

		drive-strength = <2>;
		bias-pull-down;
	};

	sdc2_cd_default: sdc2-cd-default-state {
		pins = "gpio38";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
	};
};

&pm8916_mpps {
	pwm_out: mpp4 {
		pins = "mpp4";
		function = "digital";

		output-low;
		qcom,dtest = <1>;
		power-source = <PM8916_MPP_VPH>;
	};
};
